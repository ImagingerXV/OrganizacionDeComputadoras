section .data
char db 0
newline db 10
section .text
global _start
_start:

mov al, 127
shr al, 1
ror al, 1
shl al, 1
rol al, 1
sub al, 21

mov [char], al

mov eax, 4
mov ebx, 1
mov ecx, char
mov edx, 1
int 0x80

mov eax, 4
mov ebx, 1
mov ecx, newline
mov edx, 1
int 0x80

mov eax, 1
xor ebx, ebx
int 0x80
